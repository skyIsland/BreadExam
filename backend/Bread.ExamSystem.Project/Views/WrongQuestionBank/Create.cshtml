@model Bread.ExamSystem.Project.ViewModel.WrongQuestionBankVMs.WrongQuestionBankVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.QuestionID" />
<wt:combobox field="Entity.FrameworkUserID" items="AllFrameworkUsers"/>
<wt:textbox field="Entity.WrongNumber" />
</wt:row>

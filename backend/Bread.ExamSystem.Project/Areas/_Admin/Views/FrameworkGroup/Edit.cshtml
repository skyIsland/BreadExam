@model WalkingTec.Mvvm.Mvc.Admin.ViewModels.FrameworkGroupVMs.FrameworkGroupVM

<wt:form vm="@Model">
  <wt:display field="Entity.GroupCode" />
  <wt:textbox field="Entity.GroupName" />
    <wt:tree field="Entity.ParentId" item-url="/_Admin/FrameworkGroup/GetParents"/>
  <wt:textbox field="Entity.Manager" search-url="/_Admin/FrameworkUser/GetUserById" />
<wt:textarea field="Entity.GroupRemark" />
  <wt:hidden field="Entity.ID" />
  <wt:row align=" AlignEnum.Right">
    <wt:submitbutton />
    <wt:closebutton />
  </wt:row>
</wt:form>

